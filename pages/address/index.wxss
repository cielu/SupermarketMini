page{background:#f7f7f7}.container .addrEmpty{width:100%;color:#999;font-size:30rpx;padding-top:200rpx;justify-content:center}.container .addressList{width:100%;font-size:30rpx;height:calc(100% - 100rpx);margin-top:28rpx;background:#fff;flex-direction:column}.container .addressList .addressItem:last-child{border-bottom:none}.container .addressList .addressItem{color:#696969;padding:20rpx;align-items:center;justify-content:space-between;border-bottom:1rpx solid #e9e9e9}.container .addressList .addressItem .addressType{height:24rpx;color:#ff5622;font-size:18rpx;padding:0 10rpx;border-radius:10rpx;background:#FEF1E0;align-items:center;justify-content:center}.container .addressList .addressItem .addressInfo{flex:1;flex-direction:column;align-items:flex-start;padding:10rpx 20rpx}.container .addressList .addressItem .addressInfo .phone{font-size:28rpx;align-items:center;padding-bottom:18rpx}.container .addressList .addressItem .addressInfo .address{color:#333;font-size:30rpx}.newAddressBox{height:140rpx;position:fixed;bottom:0;left:0;width:100%;align-items:center;justify-content:center;padding-bottom:20rpx;box-shadow:0 0 10px rgba(0,0,0,0.25)}.newAddressBox .newBtn{width:90%;height:90rpx;border-radius:90rpx;color:white;font-size:26rpx;justify-content:center;align-items:center;background:linear-gradient(75deg, #FF9900, #FF6600)}
