view,div{display:flex;-webkit-overflow-scrolling:touch}page{background:#f7f7f7}.container .card{width:100%;font-size:26rpx;background:#fff;flex-direction:column;margin-bottom:26rpx;padding-bottom:20rpx;box-sizing:border-box}.container .card .cell{height:100rpx;font-size:30rpx;padding-right:26rpx;margin-left:26rpx;align-items:center;justify-content:space-between;border-bottom:1rpx solid #e9e9e9}.container .card .title{padding:30rpx;font-size:30rpx}.container .card .input{width:70%;height:70rpx;text-align:right}.container .card .content{width:auto;height:300rpx;margin:26rpx;padding:20rpx;border-radius:10rpx;box-sizing:border-box;border:1rpx solid #e9e9e9}.container .card .cates{margin:0 20rpx;flex-wrap:wrap;box-sizing:border-box}.container .card .cates .cate{color:#333;margin:10rpx 14rpx;padding:14rpx 22rpx;border-radius:16rpx;border:1rpx solid #C9C9C9}.container .card .cates .active{color:#fff;background:#FF7700;border:1rpx solid #FF7700}.container .card .item{font-size:30rpx;flex-direction:column}.container .card .item .images{padding:10rpx 30rpx;flex-wrap:wrap;box-sizing:border-box}.container .card .item .images .bdn{border:none !important}.container .card .item .images .upload{width:130rpx;height:130rpx;position:relative;margin-right:20rpx;align-items:center;justify-content:center;border:1rpx dashed #c9c9c9}.container .card .item .images .upload .close{top:0;right:0;position:absolute}.container .card .item .images .upload .img{width:100%;height:100%;object-fit:contain}.container .submitBox{width:100%;padding-top:100rpx;justify-content:center}.container .submitBox .submitBtn{width:90%;height:90rpx;align-items:center;justify-content:center;font-size:28rpx;color:white;border-radius:90rpx;background:linear-gradient(75deg, #FF9900, #FF6600)}
